@model CwoPqsCrosswalk.Models.Officer

@{
    ViewData["Title"] = "Officer";
}

<h1>
    @Html.DisplayFor(model => model.Rank)
    @Html.DisplayFor(model => model.FirstName)
    @Html.DisplayFor(model => model.LastName)
</h1>

<div class="container">

    <ul class="nav nav-tabs" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" data-toggle="tab" href="#twoAlpha">2A</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" data-toggle="tab" href="#twoBravo">2B</a>
        </li>
    </ul>

    <div class="tab-content">
        <div id="twoAlpha" class="container tab-pane active">
            <br>
            <h3>NAVEDTRA 43357-2A - November 2016</h3>
            @{
                Html.RenderPartial("Pqs", Model.TwoAlpha);
            }
        </div>
        <div id="twoBravo" class="container tab-pane active">
            <br>
            <h3>NAVEDTRA 43357-2B - 14 March 2020</h3>
            @{
                Html.RenderPartial("Pqs", Model.TwoBravo);
            }
        </div>
    </div>
</div>

